<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" 
    android:orientation="vertical"
    android:layout_width="wrap_content" 
    android:layout_height="wrap_content"
    >

        
        <RelativeLayout
            android:id="@+id/rv_item_mypage_me_container"
            android:layout_width="149dp"
            android:layout_height="wrap_content"
            android:layout_margin="10.5dp"

            >
        <ImageView
            android:id="@+id/rv_item_mypage_me_img"
            android:layout_width="match_parent"
            android:layout_height="148.8dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="9.5dp"
            android:src="@drawable/mydirectory"
            android:scaleType="centerCrop"
            />

        <TextView
            android:id="@+id/rv_item_mypage_me_directory_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="모든 스크랩"
            android:layout_marginTop="12.2dp"
            android:layout_marginLeft="16.5dp"
            android:layout_below="@id/rv_item_mypage_me_img"
            android:fontFamily="@font/nanum_barungothic_regular"
            android:textSize="14sp"
            android:textColor="@color/black"
            />
        </RelativeLayout>

    

</RelativeLayout>